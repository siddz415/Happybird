{
	"name": "happybird",
	"version": "1.0.0",
	"description": "Happybird is a simple 2D game built with JavaScript and Kaboom.js.",
	"scripts": {
	  "build": "esbuild --bundle src/main.js --outfile=www/main.js --minify",
	  "dev": "esbuild --bundle src/main.js --outfile=www/main.js --servedir=www",
	  "predeploy": "npm run build",
	  "deploy": "gh-pages -d www"
	},
	"devDependencies": {
	  "esbuild": "^0.14.0",
	  "gh-pages": "^3.2.3"
	},
	"dependencies": {
	  "kaboom": "^0.5.0"
	},
	"homepage": "https://siddz415.github.io/happybird"
  }